{
    "app": "evaluationapi",
    "platform": "django",
    "disks": [
        {
            "name": "sqlite",
            "mountTo": "data"
        },
        {
            "name": "media",
            "mountTo": "media"
        }
    ],
    "cron": [
        "*/5 * * * * cd $ROOT && python3 manage.py checkEvaluationTime >> /dev/null 2>&1"
    ]
}